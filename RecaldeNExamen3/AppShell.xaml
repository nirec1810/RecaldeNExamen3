<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RecaldeNExamen3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:RecaldeNExamen3.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="RecaldeNExamen3">

    <TabBar>
        <ShellContent
            Title="Registro"
            ContentTemplate="{DataTemplate views:Registro}" 
            />

        <ShellContent
            Title="Lista"
            ContentTemplate="{DataTemplate views:ListaVehiculos}" 
            />

        <ShellContent
            Title="Log"
            ContentTemplate="{DataTemplate views:Log}" 
            />
    </TabBar>

</Shell>
